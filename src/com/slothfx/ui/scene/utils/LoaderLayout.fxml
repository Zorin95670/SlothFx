<AnchorPane style="-fx-border-color: #888888; -fx-border-width: 4; -fx-border-radius: 2;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="layout" alignment="CENTER" prefHeight="100.0" prefWidth="250.0">
         <children>
            <Label fx:id="info" text="Label">
               <padding>
                  <Insets bottom="15.0" top="15.0" />
               </padding></Label>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
